a, a:visited
    color       $link-color
    text-decoration none
a:focus, a:hover
    color       $link-color-hover
    text-decoration underline
a:focus
    background  $focus-bg-color
    box-shadow  $focus-box-shadow
    outline     $focus-border

a.dark, a.dark:visited
    color       $link-dark-color
    text-decoration none
a.dark:focus, a.dark:hover
    color       $link-dark-color-hover
    text-decoration underline