.acl-table-th { font-size:0.85em;font-weight:300;line-height:2.5em; }
.acl-table-headtr { text-align:left;background:$link-color;color:#FFF; }
.acl-table-bodytr { font-size:0.7em;line-height:2.2em;vertical-align:top; }
.acl-table-bodytr:nth-child(even) { background:#F4F4F4; }
.acl-table-bodytr:hover { background:#CBE0F5;opacity:0.8; }
.acl-table-bodytr.parent { background:#FDF1CC; }
.acl-table td, .acl-table th { padding:0 5px; }
.acl-table .cx { width:60px;position:relative; }
.acl-table .c3 div { overflow:hidden;text-overflow:ellipsis;white-space:nowrap; }
.acl-table [data-flag="f"] { background:#FF4C4C; }
.acl-table [data-flag="fi"] { background:#FFA6A6; }
.acl-table [data-flag="t"] { background:#94DB70; }
.acl-table [data-flag="ti"] { background:#CAEDB8; }
.manage-acl { position:relative; }
