.page_manage_acl
    
    #privTable .thead tr { text-align:left;background:$link-color;color:#FFF; }
    #privTable .thead th { font-size:0.85em;font-weight:300;line-height:2.5em; }
    #privTable .tbody tr { font-size:0.7em;line-height:2.2em;vertical-align:top; }
    #privTable .tbody tr:nth-child(even) { background:#F4F4F4; }
    #privTable .tbody tr:hover { background:#CBE0F5; }
    #privTable .tbody tr.parent { background:#FDF1CC; }
    #privTable td, #privTable th { padding:0 5px; }
    #privTable .c3 div { overflow:hidden;text-overflow:ellipsis;white-space:nowrap; }
    #privTable .cx { width:60px;position:relative; }
    #privTable #freezing { position:fixed;z-index:999;top:0; }
    #privTable #freezing tr { display:none;opacity:0.9; }
    #privTable .cx[data-flag="f"] { background:#FF4C4C; }
    #privTable .cx[data-flag="fi"] { background:#FFA6A6; }
    #privTable .cx[data-flag="t"] { background:#94DB70; }
    #privTable .cx[data-flag="ti"] { background:#CAEDB8; }
    #privTable tr:hover { opacity:0.9; }