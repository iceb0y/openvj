@font-face {
    font-family: 'openvj-symbols';
    src:url('../fonts/openvj-symbols.eot');
    src:url('../fonts/openvj-symbols.eot?#iefix') format('embedded-opentype'),
        url('../fonts/openvj-symbols.woff') format('woff'),
        url('../fonts/openvj-symbols.ttf') format('truetype'),
        url('../fonts/openvj-symbols.svg#openvj-symbols') format('svg');
    font-weight: normal;
    font-style: normal;
}

/* Use the following CSS code if you want to use data attributes for inserting your icons */
[data-icon]:before {
    font-family: 'openvj-symbols';
    content: attr(data-icon);
    speak: none;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

