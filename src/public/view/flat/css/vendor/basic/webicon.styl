@font-face
    font-family 'Vijos Symbols'
    font-style  normal
    font-weight 400
    src: url('../fonts/vijossymbols.eot');
    src: local('Vijos Symbols'), local('VijosSymbols'),
         url('../fonts/vijossymbols.eot') format('embedded-opentype'),
         url('../fonts/vijossymbols.woff') format('woff'),
         url('../fonts/vijossymbols.ttf')  format('truetype'),
         url('../fonts/vijossymbols.svg#VijosSymbols') format('svg');

.icon-symbol
    font-family "Vijos Symbols"
    speak       none
    display     inline-block
    line-height 1
    text-decoration none
    -webkit-font-smoothing antialiased

.icon-home:before       {content:"\0021"}
.icon-problems:before   {content:"\0022"}
.icon-discussion:before {content:"\0023"}
.icon-tags:before       {content:"\0024"}
.icon-contests:before   {content:"\0025"}
.icon-records:before    {content:"\0026"}
.icon-user:before       {content:"\0027"}
